{
	"schemaVersion": "0.1.0",
	"name": "YTEssay_DataNote",
	"behavior": "create",
	"noteContentFormat": "",
	"properties": [
		{
			"name": "Cover",
			"value": "{{schema:thumbnailUrl|slice:0}}",
			"type": "text"
		},
		{
			"name": "Category",
			"value": "YT",
			"type": "text"
		},
		{
			"name": "Published",
			"value": "{{schema:uploadDate|date: \\\"DD/MM/YY\\\"}}",
			"type": "date"
		},
		{
			"name": "Author",
			"value": "{{schema:author}}",
			"type": "multitext"
		},
		{
			"name": "Duration",
			"value": "{{schema:duration|slice:2,-1|duration:\\\"H:mm:ss\\\"}}",
			"type": "text"
		},
		{
			"name": "Video",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "Tags",
			"value": "",
			"type": "multitext"
		},
		{
			"name": "Watched",
			"value": "",
			"type": "checkbox"
		}
	],
	"triggers": [
		"https://www.youtube.com/watch?v="
	],
	"noteNameFormat": "{{schema:name}}",
	"path": "99_Assets/Data_Notes"
}